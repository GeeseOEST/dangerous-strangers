{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z]+$": {
      "type": "object",
      "properties": {
        "first": {
          "type": "array",
          "items": { "type": "string" }
        },
        "last": {
          "type": "array",
          "items": { "type": "string" }
        }
      }
    }
  }
}
